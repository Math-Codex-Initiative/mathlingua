
[\something{A}]
Defines: X
when: 'A is \something.else'
satisfying:
. 'X is \something.else'
. 'X is \some.other.thing'
written: "\textrm{something} A?"
called: "something"


[\something2{A}]
Defines: X
when: 'A is \something.else'
satisfying: 'X is \some.other.thing'
written: "\textrm{something} A?"
called: "something2"


[\some.function{A}(x)]
Defines: f(x)
when: 'A is \something'
satisfying: 'f is \function'
written: "\textrm{some.function}(x?)"
called: "some function over $A?$"


[\some.function2{A}(x)]
Defines: f(x)
satisfying: 'A is \function:on{\reals}:to{\reals}'
written: "\textrm{some.function}(x?)"
called: "some function2 over $A?$"


[\even.primes]
Defines: X := {x}
satisfying:
. 'X is \set'
. 'x is \prime'
. 'x is \even'
written: "\textbf{even primes}"
called: "even primes"


[\even.primes2]
Defines: X := {x}
satisfying:
. 'X is \set'
. 'x is \prime'
. 'x is \even'
. have: 'x is \something'
  by: '\some.theorem'
. have:
  . 'x is \something.else'
  . forAll: a, b
    suchThat: 'a, b is \something'
    then:
    . 'a, b is \something'
  by: '\some.other.theorem'
written: "\textbf{even primes}"
called: "even primes2"


[\f1(x)]
Defines: f(x)
expressing:
. 'f is \function'
. 'f(x) := x + 1'
written: "f1(x?)"
called: "f1"


[\f12(x)]
Defines: f(x)
expressing: 'f(x) := x + 1'
written: "f1(x?)"
called: "f12"


[\f3(x)]
Defines: f(x)
expressing:
. 'f is \function'
. piecewise:
  when: 'x > 0'
  then: 'f(x) := 1'
  when: 'x < 0'
  then: 'f(x) := -1'
  else: 'f(x) := 0'
written: "f3(x?)"
called: "f3"


[\f32(x)]
Defines: f(x)
expressing:
. piecewise:
  when: 'x > 0'
  then: 'f(x) := 1'
  when: 'x < 0'
  then: 'f(x) := -1'
  else: 'f(x) := 0'
written: "f3(x?)"
called: "f32"


[\semigroup]
Defines: G := (X, *)
satisfying:
. 'X is \set'
. '* is \binary.operation:on{X}'
. forAll: x, y, z
  suchThat: 'x, y, z in X'
  then: '(x*y)*z = x*(y*z)'
providing:
. view:
  as: '\set'
  via: 'X'
  by: '\some.theorem'
. membership:
  through: 'X'
written: "\textrm{semigroup}"
called: "semigroup"


[\something]
Defines: G := (X, *)
satisfying: "something"
providing:
. symbols: *
  where: 'x * y := x G.* y'
. memberSymbols: *
  where: 'x * y := x G.* y'
written: "something"


[\x]
Defines: x
means: 'x is \set'
satisfying: "something"
written: "x"


[\y]
Defines: (y, z, w)
means:
. '(y, z) is \set'
. '(y, w) is \set'
. '(z, w) is \set'
satisfying: "something"
written: "y"


[\nat]
Defines: n
satisfying:
. generated:
  from:
  . 0
  . succ(n)
written: "nat"


[\nat2]
Defines: n
satisfying:
. generated:
  from:
  . 0
  . succ(n)
  when:
  . 'n is \nat2'
written: "nat2"


[\z]
Defines: f(x)
expressing:
. matching: n
  with:
  . 'f(n) := 0'
written: "f(x)"


